    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "mkdirp": "^0.5.1",
    "node-loader": "^0.6.0",
    "proto-loader": "^0.2.0",
    "rimraf": "^2.6.1",
